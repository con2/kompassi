thead
  tr
    th Sukunimi
    th Etunimi
    th Nick
    th.hidden-xs Tehtäväalueet

tbody
  for signup in signups
    tr
      td: a(href='{% url "labour_admin_signup_view" event.slug signup.person.pk %}') {{ signup.person.surname }}
      td: a(href='{% url "labour_admin_signup_view" event.slug signup.person.pk %}') {{ signup.person.first_name }}
      td: a(href='{% url "labour_admin_signup_view" event.slug signup.person.pk %}') {{ signup.person.nick }}
      td.hidden-xs: include labour_profile_job_category_labels.jade
